{
  "name": "yaml-generator-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:docker": "ng serve --host 0.0.0.0 --poll 500",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "docker:dev": "docker build -t yaml-generator-frontend:dev --build-arg ENV=dev .",
    "docker:prod": "docker build -t yaml-generator-frontend:prod --build-arg ENV=prod .",
    "start:docker:dev": "docker run -p 4200:80 yaml-generator-frontend:dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.8",
    "@angular/cdk": "^19.2.11",
    "@angular/common": "^19.2.8",
    "@angular/compiler": "^19.2.8",
    "@angular/core": "^19.2.8",
    "@angular/forms": "^19.2.8",
    "@angular/material": "^19.2.11",
    "@angular/platform-browser": "^19.2.8",
    "@angular/platform-browser-dynamic": "^19.2.8",
    "@angular/router": "^19.2.8",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "glob": "^11.0.1",
    "js-yaml": "^4.1.0",
    "lru-cache": "^11.1.0",
    "rimraf": "^6.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular-eslint/schematics": "^19.3.0",
    "@angular/cli": "~19.2.9",
    "@angular/compiler-cli": "^19.2.8",
    "@types/js-yaml": "^4.0.9",
    "angular-eslint": "^19.3.0",
    "typescript": "~5.8.3"
  }
